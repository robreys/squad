





  <body class="o-page p-product-single">
    <div id="page">
      <div id="header">
        <div class="header-content">
          <a href="/home" class="p-link-home"><img src="/img/icons/home-24.ico" width="16px" ></a>
          
          <a href="javascript:history.back();" class="p-link-back"><img src="/img/icons/arrow-24.ico" width="16px;"></a>
        </div>
      </div>
      <div class="bannerPane">
        <div class="overlay"></div>
        <div class="s-banner-content">
          <div><img  width="100" src="/img/squad-crop.png" /></div>
          <strong>{{first_name}} {{last_name}}</strong>
        </div>
      </div>
      <div id="content">
        
        <article>

          <div class="prod-single-content">
            
            <p>{{about}}</p>
            
            <h3 class="title" style="font-size: 12px"><strong>Email</strong>: {{email}}</h3>
            <br/>
            <div class="blog-tags" >
                  <ul  style="position: relative;">
                  <li><img src="/img/icons/tag-16.ico"></li>
                  <li><strong>Tags</strong>: </li>
                        {{#each tags}}
                      <li><a href="#">{{this}}</a></li>
                        {{/each}}
          
                  </ul>
            </div>
            
            <div class="blog-tags" >
                  <ul  style="position: relative;">
                  <li><img src="/img/icons/conference-16.ico"></li>
                  <li><strong>Squads</strong>: </li>

                            {{#each squads}}
                            <li><a href="/squad/{{_id}}">{{name}}</a></li>
                            {{/each}}
                 
                  </ul>
            </div>
            <br/>
            <div class="p-single-meta-shares blog-tags" style="text-align: right; float: right;">
              {{#if admin}}
      
              <ul><li><a href="/user/{{_id}}/edit">Edit</a></li></ul>
              {{/if}}
            </div>
            <br/>
          </div>
          
        </article>

      </div>
      <div class="subFooter">A Squad Company. <a href="/" class="p-link-logout" style="font-size: 12px;"> &nbsp Logout</a></div>
      
      
      
    </div>
  </body>